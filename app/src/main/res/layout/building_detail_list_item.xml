    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="10dp"
    android:paddingBottom="10dp"
    android:paddingRight="10dp">
    <TextView
        android:id="@+id/building_detail_item"
        android:layout_width="match_parent"
